.navbar.navbar-default
  .container-fluid
    .navbar-header
      %button.navbar-toggle.collapsed{'data-toggle' => "collapse", 'data-target' => "#beauvoir-navbar-collapse", 'aria-expanded' => "false"}
        %span.sr-only Toggle Navigation
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar
    .collapse.navbar-collapse#beauvoir-navbar-collapse
      %ul.nav.navbar-nav
        %li#home
          = link_to "Home", '/home'
        %li#trainees
          = link_to "Trainees", '/trainees'
        %li#collective-members
          = link_to "Collective Members", '/collective_members'
        %li.dropdown
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Help <span class="caret"></span></a>
          %ul.dropdown-menu
            %li= link_to "Help", '/help'
            %li= link_to "About", '/about'
      %ul.nav.navbar-nav.navbar-right
        %li#logout
          = link_to "Sign Out", destroy_user_session_path, method: :delete
