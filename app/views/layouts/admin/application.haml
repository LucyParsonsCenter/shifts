!!!
%html
  %head
    %meta{:name => "ROBOTS", :content =>"NOODP"}
    %meta{:name => "viewport", :content => "initial-scale=1"}
    %title= content_for(:title)
    = stylesheet_link_tag "//fonts.googleapis.com/css?family=Lato:300,400,900", media: "all"
    = stylesheet_link_tag "administrate/application", media: "all"
    = javascript_include_tag "administrate/application", 'data-turbolinks-track' => true
    = csrf_meta_tags
  %body.administrate
    %main.main
      .sidebar
      .container
        - if user_signed_in?
          = render :partial => 'layouts/navigation'
        = content_for(:search)
        .content
          = render "flashes"
          = yield


